  "bugs": {
    "url": "https://github.com/Thirassia/testing/issues"
  },
  "homepage": "https://github.com/Thirassia/testing#readme",
  "devDependencies": {
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "mocha": "^9.1.3"
  }
}
